% O latex aceita tanto: Fulano Silva quanto Silva, Fulano como formatos para nomes de autores.
% Até 3 autores, todos são citados (separados por "and")
% Acima disto, usa-se "Fulano Silva and others" para gerar o et al. (exceto para trabalhos do próprio programa/instituto)

% livro
@book{livro, 
    author = {Fulano Silva and Ciclano Santos and Beltrano Rosa},
    title = {Título do Livro},
    year = {1998},
    publisher = {Ed. Tabajara},
    edition = {3rd},
    address = {Rio Grande, RS, Brasil}, % opcional
 }

% parte de livro/monografia (colocar páginas)
@incollection{capitulo,
    booktitle={Título do Livro},
    editor={Santos, Ciclano},
    author={Silva, Fulano and others}, 
    title={Título do Capítulo},
    year={2011},
    publisher={Ed. IFRS},
    pages={205-242},
}

% artigo em revista
@article{artigo,
    author={Fulano Silva and Ciclano Santos}, 
    journal={Proceedings of the IEEE}, 
    title={Um artigo numa revista}, 
    year={2006}, 
    month={June}, 
    volume={94}, 
    number={6}, 
    pages={1089-1120}, 
}

% paper em conferência
% colocar {[S.l.]: editora} se não souber o local da editora
% note = {(aceito para publicação)} % se ainda não foi publicado
@inproceedings{paper,
	author = {Fulano Silva and Ciclano Santos},
    booktitle={Proceedings of the 28th Nome Da ConferÊncia, NDC},
	title = {Título do Paper},
	address = {Tampa, FL, USA},
	publisher = {Porto Alegre: SBC},
    year={2014}, 
	pages = {327--330},
}

% teses (mestrado - @mastersthesis)
@phdthesis{tese,
	address = {Porto Alegre},
	author = {Ciclano Santos},
	school = {PPGC, UFRGS},
	title = {Título do Trabalho Acadêmico},
	type = {Tese (Doutorado em Ciência da Computação)},
	year = {2007}
}
	
% qualquer documento que esteja disponível online, colocar o campo note com a URL
% site
